<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- 映射的属性配置, 参考《table映射配置结构设计.tmp.xml》中的属性配置说明 -->
	<property order="0" supportCover="true" supportDelete="true" extend="" />

	<!-- 
		sql-query映射配置
		* name: 映射名
		* oldName: 旧映射名; 如果配置, 框架会删除旧映射名的sql
	 -->
	<sql-query name="" oldName="">
		<!-- content中配置具体的select sql -->
		<content></content>
		
		<!-- 参数集合, 不可为空, 否则会抛出异常 -->
		<parameters>
			<!-- 
				具体的参数
				* name: 列名, 必填, 必须在上面select sql的结果集中存在, 不能为空; 不区分大小写(建议大写, 框架会自动转大写); 不能重复配置
				* dataType: 列的数据类型, 必填, 不能为空, 目前可使用值(不区分大小写): string, number, datetime
				* required: 该参数是否必须, 可为空, 默认为false
			 -->
			<parameter name="" dataType="" required="" />		
		</parameters>	
	</sql-query>
</mapping-configuration>