<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration type="table">
	<table name="SYS_USER" class="com.douglei.test.session.SysUser">
		<columns>
			<column name="id" property="id" dataType="string" />
			<column name="name" property="name" dataType="string" />
